@{
    Layout = null;
    ViewBag.Title = "Listado de Sorteos Generados";
}
<div class="col-lg-12 no-padder" ng-controller="RaffleGeneratedController" ng-init="app.title = '@(ViewBag.Title)'">
    <div class="col-lg-12 no-padder">
        <div class="panel panel-default">
            <div class="panel-body">
                <!-- START panel-->
                <div class="col-lg-12 text-right m-b-xs no-padder">
                    <a ng-click="editSolteo()" class="btn btn-square btn-success {{moduleCanAdd}}"><i class="fa fa-plus-square"></i> Agregar Sorteo</a>
                </div>
                <div class="row-border hover ">
                    <table class="table dataTableGrid">
                        <thead>
                            <tr>
                                <th>Sorteo</th>
                                <th class="text-center">No Impresos</th>
                                <th class="text-center">Impresos</th>
                                <th class="text-center">Re-Impresos</th>
                                <th class="text-center">Premiados No Impreso</th>
                                <th class="text-center">Premiados Vendidos</th>
                                <th class="text-center">PREMIADOS</th>
                                <th class="text-center">CUADRE DE SORTEO</th>
                                <th class="text-center">HISTORICO DE PAGO</th>
                                <th class="text-center">DEUDAS</th>

                            </tr>
                        </thead>
                        <tbody>
                            <tr data-ng-repeat="solteo in solteoList">
                                <td>{{solteo.Id}} - {{solteo.Name}}</td>
                                <td class="text-center">
                                    <a href="/Reports/NoPrintedNumbesForRaffle?raffleId={{solteo.Id}}" target="_blank" title="Imprimir Numeros no impreso"><i class="fa fa-print"></i></a>
                                </td>
                                <td class="text-center">
                                    <a href="/Reports/PrintedNumbesForRaffle?raffleId={{solteo.Id}}" target="_blank" title="Imprimir Numeros no impreso"><i class="fa fa-print"></i></a>
                                </td>
                                <td class="text-center">
                                    <a href="/Reports/TicketReprinted?raffleId={{solteo.Id}}" target="_blank" title="Numeros premiados Facturados"><i class="fa fa-print"></i></a>
                                </td>
                                <td class="text-center">
                                    <a href="/Reports/NoPrintedNumbesAward?raffleId={{solteo.Id}}" target="_blank" title="Imprimir Numeros premiados no impreso"><i class="fa fa-print"></i></a>
                                </td>
                                <td class="text-center">
                                    <a href="/Reports/InvoicedNumbersAward?raffleId={{solteo.Id}}" target="_blank" title="Numeros premiados Facturados"><i class="fa fa-print"></i></a>
                                </td>
                                <td class="text-center">
                                    <a href="/Reports/NumbesAward?raffleId={{solteo.Id}}" target="_blank" title="Numeros premiados"><i class="fa fa-print"></i></a>
                                </td>
                                <td class="text-center">
                                    <a href="/Reports/RaffleGeneralOver?raffleId={{solteo.Id}}" target="_blank" title="Cuadre del sorteo"><i class="fa fa-print"></i></a>
                                </td>
                                <td class="text-center">
                                    <a href="/Reports/ReportPaymenHistory?raffleId={{solteo.Id}}" target="_blank" title="Historial de Pago"><i class="fa fa-print"></i></a>
                                </td>
                                <td class="text-center">
                                    <a href="/Reports/DebtReport?raffleId={{solteo.Id}}" target="_blank" title="Reporte de Deudas"><i class="fa fa-print"></i></a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <!-- END table-responsive-->
            </div>
        </div>
    </div>
</div>
<div class="col-lg-12 no-padder text-left m-b-lg"> 
    <a ui-sref="app.dashboard" class="mr btn btn-square btn-default"><i class="fa fa-arrow-left "></i> Salir</a>
</div>
